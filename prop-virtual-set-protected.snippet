<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
    <CodeSnippet Format="1.0.0">
        <Header>
            <Title>PROP virtual and set protected</Title>
            <Author>Paulo Le√£o</Author>
            <Description>Generates PROP virtual and set protected.</Description>
            <Shortcut>propnh</Shortcut>
        </Header>
        <Snippet>
            <Code Language="csharp">
                <![CDATA[public virtual $Type$ $Name$ { get; protected set; }]]>
            </Code>
            <Declarations>
                <Literal>
                    <ID>Type</ID>
                    <Default>int</Default>
                </Literal>
                <Literal>
                    <ID>Name</ID>
                    <Default>Name</Default>
                </Literal>
            </Declarations>
        </Snippet>
    </CodeSnippet>
</CodeSnippets>